{
  "name": "building-reactjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "humps": "^2.0.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "^16.2.0",
    "react-cookie": "^2.1.4",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "cross-env REACT_APP_VERSION=$npm_package_version npm-run-all -p watch-css start-js",
    "build": "npm run build-css && cross-env REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-import": "^2.10.0",
    "node-sass-chokidar": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "react-scripts": "1.1.1",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6"
  }
}
